{
    "compilerOptions": {
        "types": ["vite/client"],
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": "./",
        "paths": {
            "~components/*": [
                "src/components/*"
            ],
            "~services/*": [
                "src/services/*"
            ],
            "~constants/*": [
                "src/constants/*"
            ],
            "~config/*":[
                "src/config/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~layouts/*": [
                "src/layouts/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~assets/*": [
                "src/assets/*"
            ],
            "~schema/*": [
                "src/schema/*"
            ],
            "~types/*": [
                "src/types/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ],
            "~/*": [
                "src/*"
            ],
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "dist"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}